pipeline
{
    agent any
    stages
    {
        stage('continousDownload')
        {
            steps
            {
                git '  https://github.com/intelliqittrainings/maven.git' 
            }
        }
        
        stage('continousBuild')
        {
            steps
            {
                sh 'mvn package' 
            }
        }
    }
}
